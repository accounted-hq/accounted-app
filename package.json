{
    "name": "accounted-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack -p 8888",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
        "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
        "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
        "db:up": "docker compose up -d postgres",
        "db:down": "docker compose down",
        "db:reset": "docker compose down -v && docker compose up -d postgres"
    },
    "dependencies": {
        "@better-auth/cli": "^1.3.7",
        "@getbrevo/brevo": "^3.0.1",
        "@node-oauth/oauth2-server": "^5.2.1",
        "@upstash/ratelimit": "^2.0.6",
        "@vercel/analytics": "^1.5.0",
        "@vercel/kv": "^3.0.0",
        "bcrypt": "^6.0.0",
        "better-auth": "^1.3.7",
        "decimal.js": "^10.6.0",
        "drizzle-kit": "^0.31.4",
        "drizzle-orm": "^0.44.4",
        "next": "15.4.6",
        "postgres": "^3.4.7",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "validator": "^13.15.15"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/bcrypt": "^6.0.0",
        "@types/node": "^20",
        "@types/pg": "^8.15.5",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@types/validator": "^13.15.2",
        "eslint": "^9",
        "eslint-config-next": "15.4.6",
        "tailwindcss": "^4",
        "tw-animate-css": "^1.3.7",
        "tsx": "^4.20.4",
        "typescript": "^5"
    }
}
